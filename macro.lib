READ MACRO A
	LEA DX,A
	MOV 21H,10
	INT 21H
	ENDM
	
WRITE MACRO A
	LEA DX,A
	MOV AH,9
	INT 21H
	ENDM
	
CRLF MACRO
	MOV AH,2
	MOV DL,0AH
	MOV 21H
	MOV DL,0DH
	INT 21H
	ENDM
	
STACK0 MACRO A
STACK SEGMENT USE16 PARA STACK 'STACK'
	DB A
STACK ENDS
ENDM

OUT1 MACRO A
	MOV DL,A
	MOV AH,2
	INT 21H
	ENDM
	
	
	
	
	
	
	
	
	
	
	
	